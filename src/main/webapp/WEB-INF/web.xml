<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ibpworkbench</display-name>
  <context-param>
    <description>Vaadin production mode</description>
    <param-name>productionMode</param-name>
    <param-value>true</param-value>
  </context-param>
  
  <servlet>
    <servlet-name>ibpworkbench</servlet-name>
    <servlet-class>com.vaadin.terminal.gwt.server.ApplicationServlet</servlet-class>
    <init-param>
      <param-name>application</param-name>
      <param-value>org.generationcp.ibpworkbench.IBPWorkbenchApplication</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ibpworkbench</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ProjectFormFieldFactory</display-name>
    <servlet-name>ProjectFormFieldFactory</servlet-name>
    <servlet-class>org.generationcp.ibpworkbench.model.formfieldfactory.ProjectFormFieldFactory</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProjectFormFieldFactory</servlet-name>
    <url-pattern>/ProjectFormFieldFactory</url-pattern>
  </servlet-mapping>
</web-app>